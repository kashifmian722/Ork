{
    "name": "schu2/custom-css-js-for-storefront",
    "description": "Custom CSS/JS for Storefront",
    "version": "v1.0.1",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Schuja IT",
            "homepage": "https://schuja.com/"
        }
    ],
    "require": {
        "shopware/core": "^6.3",
        "shopware/storefront": "^6.3"
    },
    "suggest": {
        "shopware/storefront": "Require '6.3'.",
        "shopware/administration": "Require '6.3'. Allows you to to set up the plugin easily"
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    },
    "autoload": {
        "psr-4": {
            "Schu2\\CustomCssJsForStorefront\\": "src/"
        }
    },
    "extra": {
        "shopware-plugin-class": "Schu2\\CustomCssJsForStorefront\\Schu2CustomCssJsForStorefront",
        "plugin-icon": "src/Resources/config/plugin.png",
        "copyright": "(c) by Schuja IT",
        "label": {
            "de-DE": "Custom CSS/JS for Storefront",
            "en-GB": "Custom CSS/JS for Storefront"
        },
        "description": {
            "de-DE": "Fügen Sie einfach JavaScript-Dateien oder benutzerdefiniertes JavaScript und CSS für Shopware 6 hinzu",
            "en-GB": "Add JavaScript files or custom JavaScript and CSS easily for Shopware 6"
        },
        "manufacturerLink": {
            "de-DE": "https://schuja.com/",
            "en-GB": "https://schuja.com/"
        },
        "supportLink": {
            "de-DE": "https://schuja.com/uber-uns/",
            "en-GB": "https://schuja.com/uber-uns/"
        }
    }
}
